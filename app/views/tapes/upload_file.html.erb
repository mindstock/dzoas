<%= form_tag('/tapes/upload', multipart: true) do -%>
<!-- Main bar -->
    <div class="mainbar">
      <!-- Page heading -->
      <div class="page-head">
        <!-- Page heading -->
        <h2 class="pull-left">
          <span class="page-meta">库存-新建</span>
        </h2>


        <!-- Breadcrumb -->
        <div class="bread-crumb pull-right">
          <a href="index.html"><i class="icon-home"></i> 首页</a> 
          <!-- Divider -->
          <span class="divider">/</span> 
          <a href="#" class="bread-current">库存-新建</a>
        </div>

        <div class="clearfix"></div>

      </div>
      <!-- Page heading ends -->



      <!-- Matter -->

      <div class="matter">
        <div class="container">

          <div class="row">

            <div class="col-lg-12">


              <div class="widget wgreen">

                <div class="widget-content">
                  <div class="padd">
                    <h6>上传原材料excel</h6>
                    <hr>
                    <div class="form-group">
                      <div class="col-lg-12">
                       <%= file_field_tag 'raw_file', :class => "form-control"%>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="col-lg-offset-0 col-lg-12">
                         <%= submit_tag '提交', :class => 'btn btn-success'%>
                         <%= link_to '返回', tapes_path, :class => 'btn btn-info'%>
                     </div>
                    </div>
                  </div>
                </div>
              </div>  

            </div>

          </div>

        </div>
      </div>

    <!-- Matter ends -->

    </div>



<%end -%>